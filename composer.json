{
    "name": "baskooijmaninc/suzie-bundle",
    "type": "symfony-bundle",
    "description": "Kooijman Incorporate Holding B.V. Suzie is an Orm and FormBuilder Bundle based on symfony and bootstrap",
    "keywords": [
        "Suzie Bundle"
    ],
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Sebastiaan Kooijman",
            "email": "kooijmaninc@gmail.com",
            "role": "Owner / Creator / Main Developer"
        }
    ],
    "extra": {
        "symfony": {
            "allow-contrib": "true",
            "endpoint": [
                "https://github.com/baskooijmaninc/SuzieBundle/manifest.json"
            ]
        }
    },

    "require": {
        "symfony/http-kernel": "~7.0"
    },
    "require-dev": {
        "symfony/dependency-injection": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "KooijmanInc\\SuzieBundle\\": ""
        }
    }
}
