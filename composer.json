{
    "name": "baskooijmaninc/suzie-bundle",
    "type": "symfony-bundle",
    "description": "Kooijman Incorporate Holding B.V. Suzie-Bundle",
    "keywords": [
        "Suzie Bundle"
    ],
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Sebastiaan Kooijman",
            "email": "kooijmaninc@gmail.com",
            "role": "Owner / Creator / Main Developer"
        }
    ],
    "extra": {
        "symfony": {
            "allow-contrib": "true",
            "endpoint": [
                "https://github.com/baskooijmaninc/SuzieBundle/blob/master/manifest.json"
            ]
        }
    },
    "require": {
        "php": ">=8.2",
        "ext-mysqli": "*",
        "symfony/http-kernel": "~7.0",
        "symfony/intl": "~7.0",
        "symfony/config": "~7.0",
        "symfony/console": "~7.0",
        "symfony/yaml": "~7.0"

    },
    "require-dev": {
        "symfony/dependency-injection": "~7.0",
        "symfony/stopwatch": "~7.0",
        "psr/log": "*",
        "psr/container": "*"
    },
    "autoload": {
        "psr-4": {
            "KooijmanInc\\SuzieBundle\\": "",
            "KooijmanInc\\Suzie\\": "src/Suzie/",
            "KooijmanInc\\Suzie\\FormBuilder\\": "src/FormBuilder/",
            "KooijmanInc\\Suzie\\Model\\": "src/Model/",
            "KooijmanInc\\Suzie\\Exception\\": "src/Exception/"
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}
